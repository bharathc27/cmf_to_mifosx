    <entry>
      <name>Create Loan Details Table</name>
      <description/>
      <type>SQL</type>
      <sql>CREATE TABLE IF NOT EXISTS &#x60;loan_details&#x60; &#x28;&#xd;&#xa;	&#x60;client_code&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;office_code&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;loan_no&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;loan_external_id&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;group_no&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;center&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;client_name&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;submittedon_date&#x60; DATE NULL DEFAULT NULL,&#xd;&#xa;	&#x60;applied_amount&#x60; BIGINT&#x28;10&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;approvedon_date&#x60; DATE NULL DEFAULT NULL,&#xd;&#xa;	&#x60;approved_amount&#x60; BIGINT&#x28;10&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;type&#x60; VARCHAR&#x28;1&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;disb_srlno&#x60; INT&#x28;2&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;disbursedon_date&#x60; DATE NULL DEFAULT NULL,&#xd;&#xa;	&#x60;disbursed_amount&#x60; BIGINT&#x28;10&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;first_repayment_on&#x60; DATE NULL DEFAULT NULL,&#xd;&#xa;	&#x60;frp2&#x60; DATE NULL DEFAULT NULL,&#xd;&#xa;	&#x60;interest&#x60; INT&#x28;3&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;installments&#x60; INT&#x28;3&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;datediff&#x60; INT&#x28;3&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;datediff2&#x60; INT&#x28;3&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;repay_freq&#x60; VARCHAR&#x28;1&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;repay_every&#x60; INT&#x28;2&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;repayment_period_frequency&#x60; INT&#x28;2&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;emi1&#x60; BIGINT&#x28;6&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;emi2&#x60; BIGINT&#x28;6&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;maturity_date&#x60; DATE NULL DEFAULT NULL,&#xd;&#xa;	&#x60;product_name&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL,&#xd;&#xa;	&#x60;status&#x60; VARCHAR&#x28;100&#x29; NULL DEFAULT NULL&#xd;&#xa;&#x29;&#xd;&#xa;COLLATE&#x3d;&#x27;utf8_general_ci&#x27;&#xd;&#xa;ENGINE&#x3d;InnoDB&#xd;&#xa;&#x3b;&#xd;&#xa;</sql>
