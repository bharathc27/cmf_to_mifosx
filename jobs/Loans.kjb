    <entry>
      <name>Disburse&#x2f;Repay Details Load</name>
      <description/>
      <type>SQL</type>
      <sql>insert into disb_repay&#xd;&#xa;select concat&#x28;&#x27;https&#x3a;&#x2f;&#x2f;localhost&#x2f;mifosng-provider&#x2f;api&#x2f;v1&#x2f;loans&#x2f;&#x27;,ml.id,&#x27;&#x2f;transactions&#x3f;command&#x3d;repayment&#x27;&#x29; URL,&#xd;&#xa;concat&#x28;&#x27;&#x7b; &#xd;&#xa;&#x22;locale&#x22;&#x3a; &#x22;en&#x22;, &#xd;&#xa;&#x22;dateFormat&#x22;&#x3a; &#x22;dd MMMM yyyy&#x22;, &#xd;&#xa;&#x22;transactionDate&#x22;&#x3a; &#x22;&#x27;,date_format&#x28;dt.transaction_date,&#x27;&#x25;d &#x25;M &#x25;Y&#x22;&#x27;&#x29;,&#x27;,&#xd;&#xa;&#x22;transactionAmount&#x22;&#x3a;&#x22;&#x27;,dt.amount,&#x27;&#x22;,&#xd;&#xa;&#x22;paymentTypeId&#x22;&#x3a;&#x22;&#x27;,case &#xd;&#xa;when dt.payment_type&#x3d;&#x27;C&#x27; then &#x28;select id from m_code_value where code_value&#x3d;&#x27;Cash&#x27;&#x29;&#xd;&#xa;when dt.payment_type&#x3d;&#x27;B&#x27; then &#x28;select id from m_code_value where code_value&#x3d;&#x27;Bank&#x27;&#x29;&#xd;&#xa;end,&#x27;&#x22;,&#xd;&#xa;&#x22;note&#x22;&#x3a; &#x22;&#x22;,&#xd;&#xa;&#x22;accountNumber&#x22;&#x3a;&#x22;&#x22;,&#xd;&#xa;&#x22;checkNumber&#x22;&#x3a;&#x22;&#x27;,ifnull&#x28;dt.cheque_no,&#x27;&#x27;&#x29;,&#x27;&#x22;,&#xd;&#xa;&#x22;routingCode&#x22;&#x3a;&#x22;&#x22;, &#xd;&#xa;&#x22;receiptNumber&#x22;&#x3a;&#x22;&#x27;,ifnull&#x28;dt.receipt_no,&#x27;&#x27;&#x29;,&#x27;&#x22;,&#xd;&#xa;&#x22;bankNumber&#x22;&#x3a;&#x22;&#x27;,ifnull&#x28;dt.bank_no,&#x27;&#x27;&#x29;,&#x27;&#x22;&#xd;&#xa;&#x7d;&#x27;&#x29; as JSON,&#xd;&#xa;&#x27;application&#x2f;json&#x27; as &#x27;Content-Type&#x27;,&#xd;&#xa;&#x27;default&#x27; as &#x27;X-Mifos-Platform-TenantId&#x27;,&#xd;&#xa;ml.id loan_id,&#xd;&#xa;dt.transaction_date,&#xd;&#xa;&#x27;R&#x27; as &#x27;Type&#x27;,&#xd;&#xa;dt.office_id&#xd;&#xa;from delfix_trxn dt &#xd;&#xa;inner join m_loan ml on ml.id&#x3d;dt.loan_id&#xd;&#xa;order by ml.id&#x3b;&#xd;&#xa;&#xd;&#xa;Insert into disb_repay&#xd;&#xa;select concat&#x28;&#x27;https&#x3a;&#x2f;&#x2f;localhost&#x2f;mifosng-provider&#x2f;api&#x2f;v1&#x2f;loans&#x2f;&#x27;,l.id,&#x27;&#x3f;command&#x3d;disburse&#x27;&#x29; URL,&#xd;&#xa;concat&#x28;&#x27;&#x7b;&#xd;&#xa;&#x22;transactionAmount&#x22;&#x3a;&#x27;,round&#x28;md.principal&#x29;,&#x27;,&#xd;&#xa;&#x22;actualDisbursementDate&#x22;&#x3a;&#x22;&#x27;,date_format&#x28;md.expected_disburse_date,&#x27;&#x25;d &#x25;M &#x25;Y&#x27;&#x29;,&#x27;&#x22;, &#xd;&#xa;&#x22;fixedEmiAmount&#x22;&#x3a;&#x27;,ifnull&#x28;round&#x28;l.fixed_emi_amount&#x29;,&#x27;&#x22;&#x22;&#x27;&#x29;,&#x27;,&#xd;&#xa;&#x22;locale&#x22;&#x3a;&#x22;en&#x22;,&#xd;&#xa;&#x22;dateFormat&#x22;&#x3a;&#x22;dd MMMM yyyy&#x22;&#xd;&#xa;&#x7d;&#x27;&#x29; JSON,&#xd;&#xa;&#x27;application&#x2f;json&#x27; as &#x27;Content-Type&#x27;,&#xd;&#xa;&#x27;default&#x27; as &#x27;X-Mifos-Platform-TenantId&#x27;,&#xd;&#xa;l.id &#x27;loan_id&#x27;,date&#x28;md.expected_disburse_date&#x29; &#x27;date&#x27;,&#xd;&#xa;&#x27;D&#x27; as &#x27;Type&#x27;,&#xd;&#xa;mc.office_id from&#xd;&#xa;m_loan l &#xd;&#xa;inner join m_loan_disbursement_detail md on md.loan_id&#x3d;l.id&#xd;&#xa;inner join m_client mc on mc.id&#x3d;l.client_id&#xd;&#xa;order by 5,6</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
