    <sql>&#x2f;&#x2a;select distinct concat&#x28;app.unit_code,app.CLIENT_CODE&#x29; client_code,&#xd;&#xa;app.UNIT_CODE office_code,&#xd;&#xa;app.LOAN_NO loan_no,&#xd;&#xa;concat&#x28;concat&#x28;app.unit_code,&#x27;-&#x27;&#x29;,app.LOAN_NO&#x29; loan_external_id,&#xd;&#xa;concat&#x28;concat&#x28;app.unit_code,&#x27;-&#x27;&#x29;,app.GROUP_NO&#x29; group_no,&#xd;&#xa;vl.VILLAGE_DESC center,&#xd;&#xa;cll.CLIENT_NAME client_name,&#xd;&#xa;app.APPL_DATE submittedon_date,&#xd;&#xa;app.APPL_AMOUNT applied_amount,&#xd;&#xa;ls.SANCTION_DATE approvedon_date,&#xd;&#xa;ls.SANCTION_AMT approved_amount,&#xd;&#xa;p.DISBURSE_TYPE type,&#xd;&#xa;ds.DISB_SRLNO,&#xd;&#xa;ds.DISB_DATE disbursedon_date,&#xd;&#xa;ds.DISB_AMT disbursed_amount,&#xd;&#xa;ds.INSTALL_STRTDATE first_repayment_on,&#xd;&#xa;sc.SCHEDULE_DATE frp2,&#xd;&#xa;ls.INT_RATE interest,&#xd;&#xa;ds.NO_OF_INSTALL installments,&#xd;&#xa;ds.repay_freq,&#xd;&#xa;sc2.schedule_date-sc.SCHEDULE_DATE datediff,&#xd;&#xa;sc3.schedule_date-sc2.SCHEDULE_DATE datediff2,&#xd;&#xa;case ds.REPAY_FREQ&#xd;&#xa;when &#x27;M&#x27; then 1&#xd;&#xa;when &#x27;T&#x27; then 4&#xd;&#xa;when &#x27;B&#x27; then 4&#xd;&#xa;else 2 end as repay_every,&#xd;&#xa;case ds.REPAY_FREQ&#xd;&#xa;when &#x27;M&#x27; then 2&#xd;&#xa;else 1 end as repayment_period_frequency,&#xd;&#xa;ds.EMI_AMT emi1,&#xd;&#xa;sc.PAYABLE_AMT emi2,&#xd;&#xa;ls.FINAL_DUE_DATE maturity_date,&#xd;&#xa;p.PRODUCT_DESCRIPTION product_name,&#xd;&#xa;ls.STATUS_CODE status&#xd;&#xa;from BO301_LOAN_SUMM ls&#xd;&#xa;inner join BO503_APPLI_HDR app on app.UNIT_CODE&#x3d;ls.UNIT_CODE and app.LOAN_NO&#x3d;ls.LOAN_NO&#xd;&#xa;inner JOIN bo102_simptab_dtl loanpurpose ON loanpurpose.simptab_col &#x3d; app.activity_code AND loanpurpose.simp_tab &#x3d; &#x27;BO0107&#x27;&#xd;&#xa;inner join BO501_REGIS_HDR cll on cll.CLIENT_CODE&#x3d;app.CLIENT_CODE and cll.UNIT_CODE&#x3d;app.UNIT_CODE&#xd;&#xa;inner join BO117_VILLAGE_LINK lk on lk.UNIT_CODE&#x3d;cll.UNIT_CODE and lk.VILLAGE_CODE&#x3d;cll.VILLAGE_CODE&#xd;&#xa;inner join BO130_VILLAGE_MST vl on vl.UNIT_CODE&#x3d;lk.UNIT_CODE and vl.VILLAGE_CODE&#x3d;lk.VILLAGE_CODE&#xd;&#xa;inner join BO505_DISBURSEMENT_HDR ds on ds.UNIT_CODE&#x3d;ls.UNIT_CODE and ds.LOAN_NO&#x3d;ls.LOAN_NO&#xd;&#xa;inner join BO509_DISBSCHEDULE sc on sc.UNIT_CODE&#x3d;app.UNIT_CODE and sc.LOAN_NO&#x3d;app.LOAN_NO and sc.SCH_SRLNO&#x3d;1 and sc.disb_srlno&#x3d;1&#xd;&#xa;inner join BO509_DISBSCHEDULE sc2 on sc2.UNIT_CODE&#x3d;app.UNIT_CODE and sc2.LOAN_NO&#x3d;app.LOAN_NO and sc2.SCH_SRLNO&#x3d;2 and sc2.disb_srlno&#x3d;1&#xd;&#xa;inner join BO509_DISBSCHEDULE sc3 on sc3.UNIT_CODE&#x3d;app.UNIT_CODE and sc3.LOAN_NO&#x3d;app.LOAN_NO and sc3.SCH_SRLNO&#x3d;3 and sc3.disb_srlno&#x3d;1&#xd;&#xa;&#xd;&#xa;INNER JOIN&#xd;&#xa;          &#x28;SELECT   MAX &#x28;id_no&#x29; AS idno, village_code AS vlcode,&#xd;&#xa;                          unit_code AS unitcode&#xd;&#xa;                     FROM bo117_village_link&#xd;&#xa;                 GROUP BY unit_code, village_code&#x29; a ON a.unitcode &#x3d; lk.unit_code AND a.vlcode &#x3d; lk.village_code  AND a.idno &#x3d; lk.id_no&#xd;&#xa;&#xd;&#xa;INNER JOIN BO105_PRODUCT_MST p on p.UNIT_CODE&#x3d;ls.UNIT_CODE and p.PRODUCT_CODE&#x3d;ls.PRODUCT_CODE and p.PROD_REV_CODE&#x3d;ls.PROD_REV_CODE&#xd;&#xa;where STATUS_CODE in &#x28;&#x27;003&#x27;,&#x27;004&#x27;,&#x27;005&#x27;,&#x27;006&#x27;,&#x27;009&#x27;&#x29;&#xd;&#xa;--and p.DISBURSE_TYPE&#x3d;&#x27;S&#x27;&#xd;&#xa;--and app.unit_code&#x3d;010 and app.loan_no&#x3d;0005976&#xd;&#xa;--and app.unit_code&#x3d;006 and app.LOAN_NO&#x3d;0006566&#xd;&#xa;--and ds.REPAY_FREQ in &#x28;&#x27;T&#x27;&#x29;--,&#x27;F&#x27;,&#x27;M&#x27;,&#x27;T&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;--and cll.UNIT_CODE&#x3d;041&#xd;&#xa;--and p.PRODUCT_DESCRIPTION&#x3d;&#x27;2 WHEELER LOAN FOR CHAITANYA&#x27;&#xd;&#xa;--and p.PRODUCT_DESCRIPTION not in &#x28;&#x27;1 YEAR GL MAX&#x27;,&#x27;1 YEAR GL&#x27;,&#x27;1 YEAR GL MIN&#x27;,&#x27;180 DAYS GL MAX&#x27;,&#x27;180 DAYS GL&#x27;,&#x27;180 DAYS GL MIN&#x27;,&#x27;90 DAYS GL&#x27;,&#x27;90 DAYS GL MIN&#x27;,&#x27;90 DAYS GL MAX&#x27;&#x29;&#xd;&#xa;order by 4,ds.disb_srlno&#xd;&#xa;--2,5,ls.UNIT_CODE,ls.LOAN_NO&#xd;&#xa;--ds.REPAY_FREQ&#x2a;&#x2f;&#xd;&#xa;&#xd;&#xa;select distinct concat&#x28;app.unit_code,app.CLIENT_CODE&#x29; client_code,&#xd;&#xa;app.UNIT_CODE office_code,&#xd;&#xa;app.LOAN_NO loan_no,&#xd;&#xa;concat&#x28;concat&#x28;app.unit_code,&#x27;-&#x27;&#x29;,app.LOAN_NO&#x29; loan_external_id,&#xd;&#xa;concat&#x28;concat&#x28;app.unit_code,&#x27;-&#x27;&#x29;,app.GROUP_NO&#x29; group_no,&#xd;&#xa;vl.VILLAGE_DESC center,&#xd;&#xa;cll.CLIENT_NAME client_name,&#xd;&#xa;app.APPL_DATE submittedon_date,&#xd;&#xa;app.APPL_AMOUNT applied_amount,&#xd;&#xa;ls.SANCTION_DATE approvedon_date,&#xd;&#xa;ls.SANCTION_AMT approved_amount,&#xd;&#xa;p.DISBURSE_TYPE type,&#xd;&#xa;ds.DISB_SRLNO,&#xd;&#xa;ds.DISB_DATE disbursedon_date,&#xd;&#xa;ds.DISB_AMT disbursed_amount,&#xd;&#xa;ds.INSTALL_STRTDATE first_repayment_on,&#xd;&#xa;sc.SCHEDULE_DATE frp2,&#xd;&#xa;ls.INT_RATE interest,&#xd;&#xa;ds.NO_OF_INSTALL installments,&#xd;&#xa;ds.repay_freq,&#xd;&#xa;sc2.schedule_date-sc.SCHEDULE_DATE datediff,&#xd;&#xa;sc3.schedule_date-sc2.SCHEDULE_DATE datediff2,&#xd;&#xa;case ds.REPAY_FREQ&#xd;&#xa;when &#x27;M&#x27; then 1&#xd;&#xa;when &#x27;T&#x27; then 4&#xd;&#xa;when &#x27;B&#x27; then 4&#xd;&#xa;else 2 end as repay_every,&#xd;&#xa;case ds.REPAY_FREQ&#xd;&#xa;when &#x27;M&#x27; then 2&#xd;&#xa;else 1 end as repayment_period_frequency,&#xd;&#xa;ds.EMI_AMT emi1,&#xd;&#xa;sc.PAYABLE_AMT emi2,&#xd;&#xa;ls.FINAL_DUE_DATE maturity_date,&#xd;&#xa;p.PRODUCT_DESCRIPTION product_name,&#xd;&#xa;ls.STATUS_CODE status,&#xd;&#xa;h.amount as lpf,&#xd;&#xa;i.amount as insurance&#xd;&#xa;from BO301_LOAN_SUMM ls&#xd;&#xa;inner join BO503_APPLI_HDR app on app.UNIT_CODE&#x3d;ls.UNIT_CODE and app.LOAN_NO&#x3d;ls.LOAN_NO&#xd;&#xa;inner JOIN bo102_simptab_dtl loanpurpose ON loanpurpose.simptab_col &#x3d; app.activity_code AND loanpurpose.simp_tab &#x3d; &#x27;BO0107&#x27;&#xd;&#xa;inner join BO501_REGIS_HDR cll on cll.CLIENT_CODE&#x3d;app.CLIENT_CODE and cll.UNIT_CODE&#x3d;app.UNIT_CODE&#xd;&#xa;inner join BO117_VILLAGE_LINK lk on lk.UNIT_CODE&#x3d;cll.UNIT_CODE and lk.VILLAGE_CODE&#x3d;cll.VILLAGE_CODE&#xd;&#xa;inner join BO130_VILLAGE_MST vl on vl.UNIT_CODE&#x3d;lk.UNIT_CODE and vl.VILLAGE_CODE&#x3d;lk.VILLAGE_CODE&#xd;&#xa;inner join BO505_DISBURSEMENT_HDR ds on ds.UNIT_CODE&#x3d;ls.UNIT_CODE and ds.LOAN_NO&#x3d;ls.LOAN_NO&#xd;&#xa;inner join BO509_DISBSCHEDULE sc on sc.UNIT_CODE&#x3d;app.UNIT_CODE and sc.LOAN_NO&#x3d;app.LOAN_NO and sc.SCH_SRLNO&#x3d;1 and sc.disb_srlno&#x3d;1&#xd;&#xa;inner join BO509_DISBSCHEDULE sc2 on sc2.UNIT_CODE&#x3d;app.UNIT_CODE and sc2.LOAN_NO&#x3d;app.LOAN_NO and sc2.SCH_SRLNO&#x3d;2 and sc2.disb_srlno&#x3d;1&#xd;&#xa;inner join BO509_DISBSCHEDULE sc3 on sc3.UNIT_CODE&#x3d;app.UNIT_CODE and sc3.LOAN_NO&#x3d;app.LOAN_NO and sc3.SCH_SRLNO&#x3d;3 and sc3.disb_srlno&#x3d;1&#xd;&#xa;left join &#x28;select x.unit_code,x.loan_no,sum&#x28;x.amount&#x29; amount from BO507_TRANSACENTRY x where x.trans_code&#x3d;&#x27;0001&#x27;and x.glac_code&#x3d;&#x27;3003&#x27; group by x.unit_code,x.loan_no&#x29; h on h.unit_code&#x3d;ds.unit_code and h.loan_no&#x3d;ds.LOAN_NO&#xd;&#xa;left join &#x28;select x.unit_code,x.loan_no,sum&#x28;x.amount&#x29; amount from BO507_TRANSACENTRY x where x.trans_code&#x3d;&#x27;00015&#x27;and x.glac_code&#x3d;&#x27;1956&#x27; group by x.unit_code,x.loan_no&#x29; i on i.unit_code&#x3d;ds.unit_code and i.loan_no&#x3d;ds.LOAN_NO&#xd;&#xa;&#xd;&#xa;INNER JOIN&#xd;&#xa;          &#x28;SELECT   MAX &#x28;id_no&#x29; AS idno, village_code AS vlcode,&#xd;&#xa;                          unit_code AS unitcode&#xd;&#xa;                     FROM bo117_village_link&#xd;&#xa;                 GROUP BY unit_code, village_code&#x29; a ON a.unitcode &#x3d; lk.unit_code AND a.vlcode &#x3d; lk.village_code  AND a.idno &#x3d; lk.id_no&#xd;&#xa;&#xd;&#xa;INNER JOIN BO105_PRODUCT_MST p on p.UNIT_CODE&#x3d;ls.UNIT_CODE and p.PRODUCT_CODE&#x3d;ls.PRODUCT_CODE and p.PROD_REV_CODE&#x3d;ls.PROD_REV_CODE&#xd;&#xa;where STATUS_CODE in &#x28;&#x27;002&#x27;,&#x27;001&#x27;,&#x27;003&#x27;,&#x27;004&#x27;,&#x27;005&#x27;,&#x27;006&#x27;,&#x27;009&#x27;&#x29;&#xd;&#xa;--and p.DISBURSE_TYPE&#x3d;&#x27;S&#x27;&#xd;&#xa;--and app.unit_code&#x3d;010 and app.loan_no&#x3d;0005976&#xd;&#xa;--and app.unit_code&#x3d;006 and app.LOAN_NO&#x3d;0006566&#xd;&#xa;--and ds.REPAY_FREQ in &#x28;&#x27;T&#x27;&#x29;--,&#x27;F&#x27;,&#x27;M&#x27;,&#x27;T&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;--and cll.UNIT_CODE&#x3d;041&#xd;&#xa;--and p.PRODUCT_DESCRIPTION&#x3d;&#x27;2 WHEELER LOAN FOR CHAITANYA&#x27;&#xd;&#xa;--and p.PRODUCT_DESCRIPTION not in &#x28;&#x27;1 YEAR GL MAX&#x27;,&#x27;1 YEAR GL&#x27;,&#x27;1 YEAR GL MIN&#x27;,&#x27;180 DAYS GL MAX&#x27;,&#x27;180 DAYS GL&#x27;,&#x27;180 DAYS GL MIN&#x27;,&#x27;90 DAYS GL&#x27;,&#x27;90 DAYS GL MIN&#x27;,&#x27;90 DAYS GL MAX&#x27;&#x29;&#xd;&#xa;order by 4,ds.disb_srlno&#xd;&#xa;--2,5,ls.UNIT_CODE,ls.LOAN_NO&#xd;&#xa;--ds.REPAY_FREQ</sql>
